<div class="global_highs">
	<table>
	<tr>
		<th>Rank</th>
		<th>Name</th>
		<th>Yos</th>
	</tr>
		<% users.each_with_index do |user,index| %>
			<tr>
				<td><%= index + 1 %></td>
			    <td><%= user.username %></td>
			    <td><%= user.yo_count %></td>
		    </tr>
		<% end %>
	</table>
	<br>
</div>
